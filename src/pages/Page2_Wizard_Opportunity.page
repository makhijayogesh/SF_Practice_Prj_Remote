<apex:page controller="Wizard_Demo">
  <apex:form >
  <apex:pageblock title="Enter Opportunity Details">
      <apex:pageBlockButtons >
          <apex:commandButton value="Previous Page" action="{!goto_Page2_Wizard_Contact}"/> 
          <apex:commandButton value="Next Page" action="{!goto_Page2_Wizard_Review}"/> 
      </apex:pageBlockButtons>
       <apex:pageblockSection columns="1">
      <apex:inputfield value="{!o.Name}"/>
      <apex:inputfield value="{!o.stageName}"/>
      <apex:inputfield value="{!o.Amount}"/>
      <apex:inputfield value="{!o.CloseDate}"/>
      <apex:inputfield value="{!o.Type}"/>
      </apex:pageblockSection>
      
      
  </apex:pageblock>
  </apex:form>
  
</apex:page>